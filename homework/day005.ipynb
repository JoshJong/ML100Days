   "source": [
    "#產生一個 1-11 的一維陣列，並且把 3-6 由正數變成負數。\n",
    "a=np.arange(12)\n",
    "print(a)\n",
    "\n",
    "for i in range(3,7):    \n",
    "    a[a==i] = -1 * a[a==i] \n",
    "\n",
    "print(a)"
   ]
